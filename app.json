{
    "name": "Adel-Userbot",
    "description": "Adel-userbot Dibuat Untuk String Telethon dan Pyrogram userbot yang stabil dan simpel.",
    "logo": "https://telegra.ph/file/2784ee65894aae04a1115.jpg",
    "keywords": [
        "Telethon",
        "Pyrogram"
    ],
    "repository": "https://github.com/mikeel-ye/Adel-Userbot",
    "success_url": "https://t.me/Darenrorr",
    "stack": "container",
    "env": {
        "API_ID": {
            "description": "ambil di my.telegram.org",
            "required": true
        },
        "API_HASH": {
            "description": "ambil di my.telegram.org",
            "required": true
        },
        "SESSION": {
            "description": "Silakan isi Telethon Atau Pyrogram V2",
            "value": ""
        },
        "HEROKU_API": {
            "description": "Wajib isi ya bre.",
            "required": true
        },
        "HEROKU_APP_NAME": {
            "description": "Isi Coy Jan Dikosongin",
            "value": "",
            "required": true
        }
    },
  "addons": [
    {
      "plan": "rediscloud",
      "options": {
        "maxmemory-policy": "allkeys-lru",
        "maxmemory-redis": "2GB"
      }
    }
  ],
  "buildpacks": [
    {
      "url": "heroku/python"
    },
    {
      "url": "https://github.com/jonathanong/heroku-buildpack-ffmpeg-latest.git"
    }
  ],
    "formation": {
        "worker": {
            "quantity": 1,
            "size": "basic"
        }
    }
}
